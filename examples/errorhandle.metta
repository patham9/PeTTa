(= (divide  $d $a)
(if (== $a 0)
  (Error ($a) (divsion by zero error))
    (/ $d $a)
))
(= (add  $a $b)
(if (not (== (get-metatype $a) Grounded))
  (Error  ($a) BadType)
    (+ $a $b)
))

!(test (divide 4 2) 2) 
!(test (divide 4 0) (Error (0) (divsion by zero error)))
!(test (add 3 2) 5)
!(test (add 3f 2) (Error  (3f) BadType))
